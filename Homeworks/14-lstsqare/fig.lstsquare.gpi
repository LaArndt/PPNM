set terminal pdf
set output "lstsquare.pdf"
set size 6
set key out 
set tics out
set xlabel "$time$"
set ylabel "$activiy$" 
set grid
plot\
 "Out.txt" index 1 using 1:2:3 with errorbars title "data"\ 
,"Out.txt" index 4 using 1:2 with lines pointtype 20 title "ls-fit"
,"Out.txt" index 9 using 1:2 with lines pointtype 20 title "ls-fit -"
,"Out.txt" index 10 using 1:2 with lines pointtype 20 title "ls-fit +"
